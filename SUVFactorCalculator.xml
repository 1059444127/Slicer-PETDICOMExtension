      <label>Height Units</label>
      <channel>output</channel>
      <description><![CDATA[. ]]></description>
    </string>
    <string>
      <name>volumeUnits</name>
      <label>Volume Units</label>
      <channel>output</channel>
      <description><![CDATA[. ]]></description>
    </string>
    <string>
      <name>injectedDose</name>
      <label>Injected Dose</label>
      <channel>output</channel>
      <description><![CDATA[. ]]></description>
    </string>
    <string>
